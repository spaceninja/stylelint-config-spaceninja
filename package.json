{
  "name": "stylelint-config-spaceninja",
  "version": "15.0.0",
  "description": "Spaceninja's stylelint config",
  "keywords": [
    "stylelint",
    "stylelint-config",
    "spaceninja"
  ],
  "repository": "spaceninja/stylelint-config-spaceninja",
  "license": "MIT",
  "author": "Scott Vandehey",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "lint": "run-s lint:js lint:prettier",
    "lint:check": "run-s lint:*:check",
    "lint:js": "eslint --fix .",
    "lint:js:check": "eslint .",
    "lint:prettier": "prettier --write .",
    "lint:prettier:check": "prettier --list-different .",
    "test": "node --test",
    "watch": "npm test --ignore-scripts -- --watch"
  },
  "prettier": "@stylelint/prettier-config",
  "dependencies": {
    "stylelint-config-standard-scss": "15.0.1",
    "stylelint-declaration-block-no-ignored-properties": "2.8.0",
    "stylelint-high-performance-animation": "1.11.0",
    "stylelint-order": "6.0.4"
  },
  "devDependencies": {
    "@stylelint/prettier-config": "3.0.0",
    "eslint": "9.35.0",
    "eslint-config-stylelint": "23.0.0",
    "npm-run-all2": "8.0.4",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "stylelint": "16.24.0"
  },
  "peerDependencies": {
    "stylelint": "^16.0.0"
  },
  "engines": {
    "node": ">=18.12.0"
  }
}
